{
  "expo": {
    "name": "dvinveste",
    "slug": "dvinveste",
    "version": "1.0.0",
    "icon": "./src/assets/images/coin.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#120423"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "splash": {
        "image": "./src/assets/images/splash.png",
        "resizeMode": "contain",
        "backgroundColor": "#120423"
      },
      "flipper": true
    },
    "plugins": [
      "expo-build-properties"
    ]
  }
}
